<div class="innerContainer"> 
<%= link_to "Add a Category", new_category_path, id: "new_category_link", remote: true %>
<br><br>

<ul id="category_list">
<% @categories.each do |cat| %>
<li><%= cat.name %>
<%= link_to "Delete", cat, method: :delete, class: "delete_link", remote: true %></li>

<% end %>
</ul>
</div>

